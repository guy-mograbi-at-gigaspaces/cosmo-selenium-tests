<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <import resource="classpath:webui-test-beans-context.xml"/>
    <context:annotation-config/>

    <bean id="book" class="gs.ui.tests.cosmo.HardCopy"/>

    <bean id="library" class="gs.ui.tests.cosmo.Library">
        <property name="book" ref="book"/>
    </bean>

    <bean id="blueprintPage" class="gs.ui.tests.cosmo.pages.BlueprintsPage">

    </bean>

    <bean id="searchPage" class="gs.ui.tests.cosmo.pages.GoogleSearchPage">

    </bean>

    <bean id="testConfiguration" class="gs.ui.tests.cosmo.TestConfiguration">
        <property name="retries" value="3"/>
    </bean>

    <context:component-scan base-package="example.pages"/>
</beans>